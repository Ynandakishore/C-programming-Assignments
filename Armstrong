#include<stdio.h>  
 int main()    
{    
    int a,b,sum=0,temp;
    printf("Enter the number which need to check :");
    scanf("%d",&a);
    temp=a;
    while(a>0)
    {
        b=a%10;
        sum=sum+(b*b*b);
        a=a/10;
    }
    if(temp==sum)
    {
        printf("the given number is armstrong");
    }
    else
    {
        printf("the given number is not an armstrong ");
    }
    return 0;
}
